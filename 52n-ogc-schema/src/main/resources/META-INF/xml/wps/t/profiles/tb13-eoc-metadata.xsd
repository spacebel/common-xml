<schema xmlns:atom="http://www.w3.org/2005/Atom" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ows="http://www.opengis.net/ows/2.0" xmlns:eoc="http://www.opengis.net/wps/2.0/profile/tb13/eoc" targetNamespace="http://www.opengis.net/wps/2.0/profile/tb13/eoc" elementFormDefault="qualified" version="2.0.0" xml:lang="en">
	<import namespace="http://www.opengis.net/ows/2.0" schemaLocation="http://schemas.opengis.net/ows/2.0/owsAll.xsd"/>
	<import namespace="http://www.w3.org/2005/Atom" schemaLocation="./atom.xsd"/>
	<element name="ApplicationContext" substitutionGroup="ows:AbstractMetaData">
		<annotation>
			<documentation>Application context consisting of an Atom feed.</documentation>
		</annotation>
		<complexType>
			<sequence>
				<element ref="atom:feed"/>
			</sequence>
		</complexType>
	</element>
</schema>
